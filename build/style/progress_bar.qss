QProgressBar {
    border: 2px solid #C0C0C0;
    border-radius: 5px;
    text-align: center;
    background-color: #f4faff;
    color: #333;
}
QProgressBar::chunk {
    background-color: #57A0D3;
    width: 20px;
    margin: 0px;
}
QProgressBar::chunk:indeterminate {
    background: qlineargradient(x1: 0, y1: 0.5, x2: 1, y2: 0.5, stop: 0 #57A0D3, stop: 1 #1034A6);
    border-radius: 5px;
}